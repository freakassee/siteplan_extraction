html
	head
		title Image Upload
	body
		h1 Lageplan Auswahl und Upload
		form( id = 'uploadForm'  enctype = 'multipart/form-data' 
			| action = '/upload' method = 'post')

			input(type = 'file' name = 'userImage' accept = '.jpg')
			input(type = 'submit' value = 'Upload image' name = 'submit')